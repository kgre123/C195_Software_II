<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.TestFirstScreen">
   <children>
      <Label fx:id="welcomeLabel" alignment="CENTER" layoutY="42.0" prefHeight="17.0" prefWidth="600.0" text="Welcome!" textAlignment="CENTER">
         <font>
            <Font name="System Bold" size="36.0" />
         </font>
      </Label>
      <Button fx:id="customerButton" layoutX="210.0" layoutY="286.0" mnemonicParsing="false" onAction="#onActionCustomerScreen" text="Customers" />
      <Button fx:id="appointmentButton" layoutX="318.0" layoutY="286.0" mnemonicParsing="false" onAction="#onActionAppointmentScreen" text="Appointments" />
      <Label fx:id="instructionLabel" alignment="CENTER" layoutY="242.0" prefHeight="17.0" prefWidth="600.0" text="Please select an option to view">
         <font>
            <Font name="System Bold" size="12.0" />
         </font>
      </Label>
      <Button fx:id="exitButton" layoutX="283.0" layoutY="325.0" mnemonicParsing="false" onAction="#onActionExitProgram" text="Exit" />
   </children>
</AnchorPane>
